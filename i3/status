general {
        colors = false
        interval = 10
}

order += "wireless _first_"
order += "battery all"
order += "tztime local"

wireless _first_ {
        format_up = "Wifi: %essid%quality"
        format_down = "W: down"
}

battery all {
        format = "%status %percentage"
        last_full_capacity = true
        integer_battery_capacity = true
}

tztime local {
        format = "%Y-%m-%d %H:%M"
}
